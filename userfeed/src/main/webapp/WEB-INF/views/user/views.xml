<!DOCTYPE tiles-definitions PUBLIC
      "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
      "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
  
<tiles-definitions>
   <definition name="user/reg" extends="home-template">
     <put-attribute name="content">
     	<definition extends="myaccount-content-template">
     		<put-attribute name="content" value="/WEB-INF/views/user/reg.jsp" />
     	</definition>
     </put-attribute>
     <put-list-attribute name="items" inherit="true">
	    <add-attribute value="/WEB-INF/views/user/reg_js.jsp" />
	  </put-list-attribute>
   </definition>
   <definition name="user/regSuccess" extends="home-template">
     <put-attribute name="content">
     	<definition extends="myaccount-content-template">
     		<put-attribute name="content" value="/WEB-INF/views/user/regSuccess.jsp" />
     	</definition>
     </put-attribute>
   </definition>
   <definition name="user/list" extends="home-template">
     <put-attribute name="content">
     	<definition extends="myaccount-content-template">
     		<put-attribute name="content" value="/WEB-INF/views/user/userList.jsp" />
     	</definition>
     </put-attribute>
   </definition>
</tiles-definitions>
